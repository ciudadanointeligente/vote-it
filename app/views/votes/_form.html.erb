<%= simple_form_for(@vote) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :organization %>
    <%= f.input :session %>
    <%= f.input :date %>
    <%= f.input :bill %>
    <%= f.input :subject %>
    <%= f.input :quorum %>
    <%= f.input :type %>
    <%= f.input :stage %>
    <%= f.input :result %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
